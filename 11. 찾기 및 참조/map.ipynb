    "import pandas as pd\n",
    "\n",
    "df = pd.read_excel(\"map_sample_data.xlsx\", \"Sheet1\")\n",
    "\n",
    "mapping = {\n",
    "    \"1\":\"BLDC\",\n",
    "    \"2\":\"인버터\",\n",
    "    \"3\":\"기타\"\n",
    "}\n",
    "\n",
    "#방법1\n",
    "df[\"제품코드_끝자리\"] = df[\"제품코드\"].str[-1]\n",
    "df[\"모터방식\"] = df[\"제품코드_끝자리\"].map(mapping)\n",
